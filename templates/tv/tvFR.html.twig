{% extends 'base.html.twig' %}
{% block body %}

    <table>

        <thead>
        <tr>
            <th colspan="3">Chaîne</th>
        </tr>
        </thead>
        <tbody>
        <tr>
            <th>numéro</th>
            <th>nom</th>
            <th>date de création</th>
